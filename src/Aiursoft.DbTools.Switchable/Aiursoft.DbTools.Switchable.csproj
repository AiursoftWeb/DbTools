    <ProjectReference Include="..\Aiursoft.DbTools.Sqlite\Aiursoft.DbTools.Sqlite.csproj" />
    <None Include="..\..\Readme.md" Pack="true" PackagePath="." />
  </ItemGroup>
</Project>