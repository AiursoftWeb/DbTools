<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<!--Build and code-->
		<OutputType>Library</OutputType>
		<Version>6.0.30</Version>
		<TargetFramework>net6.0</TargetFramework>
		<!--Best practice-->
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<!--Nuget-->
		<Company>Aiursoft</Company>
		<Product>DbTools</Product>
		<Authors>anduin</Authors>
		<Description>A tool for database.</Description>
		<PackageTags>nuget dotnet csproj dependencies database</PackageTags>
		<RepositoryType>git</RepositoryType>
		<RepositoryUrl>https://gitlab.aiursoft.cn/aiursoft/dbtools</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://gitlab.aiursoft.cn/aiursoft/dbtools</PackageProjectUrl>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Aiursoft.CSTools" Version="6.0.24" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="6.0.19" />
		<PackageReference Include="Microsoft.Extensions.Hosting" Version="6.0.1" />
		
	</ItemGroup>

</Project>
