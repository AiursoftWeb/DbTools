<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <Version>7.0.4</Version>
    <TargetFramework>net7.0</TargetFramework>
    <AssemblyName>Aiursoft.DbTools.InMemory</AssemblyName>
    <RootNamespace>Aiursoft.DbTools.InMemory</RootNamespace>
    <IsTestProject>false</IsTestProject>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IsPackable>true</IsPackable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Company>Aiursoft</Company>
    <Product>DbTools</Product>
    <Authors>anduin</Authors>
    <Description>A tool for database.</Description>
    <PackageId>Aiursoft.DbTools.InMemory</PackageId>
    <PackageTags>nuget dotnet csproj dependencies database</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://gitlab.aiursoft.cn/aiursoft/dbtools</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://gitlab.aiursoft.cn/aiursoft/dbtools</RepositoryUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.InMemory" Version="7.0.13" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Aiursoft.DbTools\Aiursoft.DbTools.csproj" />
  </ItemGroup>
</Project>